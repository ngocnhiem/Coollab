// To learn how to write nodes, see https://coollab-art.com/Tutorials/Writing%20Nodes/Intro

INPUT UV->float 'Image';
INPUT float 'Brightness';
INPUT float 'Seed';

float main(UV uv)
{
    float grey = 'Image'(uv);
    return grey < pow(hash_0_to_1_3D_to_1D(vec3(uv, 'Seed')), 'Brightness') ? 0. : 1.;
}